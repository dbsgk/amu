package thread;
class JoinTest implements Runnable {
	//0.5초씩 마다 1,2,3,4,5 출력

	@Override
	public void run() {
		for(int i=1; i<=5;i++) {
			System.out.println(i);
		}
	}
	
}
//-------------------
public class JoinMain {
	public static void main(String[] args) {
		//thread 생성, 시작
		JoinTest jt = new JoinTest();
		jt.start(500);
	}
}


