enum EnumColar{
	RED,GREEN,BLUE,MAGENTA //하나의 상수값으로 쓰겠다고 선언하는거임
}
//------------------------
public class EnumMain {
	public static void main(String[] args) {
		System.out.println(EnumColar.RED);
		System.out.println();
		
	}
}
