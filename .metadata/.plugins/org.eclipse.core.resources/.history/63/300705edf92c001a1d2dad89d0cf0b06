package inheritance;

class AA{}
class BB extends AA{}

public class InstanceOf {

	public static void main(String[] args) {
		AA aa = new AA();
		AA bb = aa; //주소값 넘겨주는 것(같은 타입끼리)
		
		//instanceOf: 오른쪽 값을 왼쪽으로 넣을 수 있는 지 확인.
		//aa를 bb에 넣기 전에 bb의
		if( aa instanceof AA) System.out.println("1. TRUE");
		else System.out.println("1. FALSE");
	}

}
