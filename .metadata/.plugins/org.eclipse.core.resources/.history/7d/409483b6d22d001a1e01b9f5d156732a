package abstract_1;

/*class Object{
	public String toString() {
		
	}
	
  class String extends Object{
  	public String toString(){}//巩磊凯
  }
}*/
class Test{
	public String toString() {
		return getClass()+"救崇";
}
}
//-------------------
public class ObjectMain {
	public static void main(String[] args) {
		Test t = new Test();
		System.out.println("按眉 t ="+t);
		System.out.println("按眉 t ="+t.toString());//按眉 t =abstract_1.Test@15db9742
		System.out.println("按眉 t ="+t.hashCode());//林家蔼16-->10柳荐肺 茄 巴
		System.out.println();
		
		String str = "apple";
		System.out.println("按眉 str ="+str);
		System.out.println("按眉 str ="+str.toString());//按眉 t =abstract_1.Test@15db9742
		System.out.println("按眉 str ="+str.hashCode());//林家蔼16-->10柳荐肺 茄 巴
		System.out.println();
	}


}
