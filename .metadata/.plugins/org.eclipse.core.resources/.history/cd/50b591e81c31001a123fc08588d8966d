package collection;

import java.util.Arrays;

public class PersonSort {
	public static void main(String[] args) {
		String[] ar = {"orange","apple","banana","pear","peach","applemango"};
		//? Comparable
		
		
		System.out.print("정렬 전 = ");
		for(String data : ar) {
			System.out.print(data+" ");
		}
		System.out.println();
		
		Arrays.sort(ar);
		System.out.print("정렬 후= ");
		for(String data : ar) {
			System.out.print(data+" ");
		}
	}//main
}//class
