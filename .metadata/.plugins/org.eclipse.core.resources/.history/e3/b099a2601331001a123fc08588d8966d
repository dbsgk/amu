package collection;

import java.util.ArrayList;
import java.util.Iterator;

public class PersonMain2 {
	public ArrayList<PersonDTO> init() {
		ArrayList<PersonDTO> dto = new ArrayList<>();
		PersonDTO aa = new PersonDTO("홍길동",25);
		PersonDTO bb = new PersonDTO("또치",40);
		PersonDTO cc = new PersonDTO("코난",16);
		dto.add(aa);
		dto.add(bb);
		dto.add(cc);
		
		return dto;
		
	}
	public static void main(String[] args) {
		PersonMain2 main = new PersonMain2();
		ArrayList<PersonDTO> a = main.init();
		for(PersonDTO d : a) {
			System.out.println("이름="+d.getName()+"  나이="+d.getAge());
		}
	}
}
